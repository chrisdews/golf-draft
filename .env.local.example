# Copy this file to .env.local and fill in the values

# RapidAPI key for golf leaderboard data (https://rapidapi.com/golf-leaderboard-data)
NEXT_PUBLIC_API_KEY=

# Firebase config (from Firebase console → Project settings → Your apps)
NEXT_PUBLIC_FIREBASE_KEY=
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=
NEXT_PUBLIC_STORAGE_BUCKET=

# Set to "mock" to use hardcoded local data instead of live API calls (no API key required)
# NEXT_PUBLIC_MOCK_ENV=mock
